<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servletAdmin</servlet-name>
        <servlet-class>controladores.servletAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>controladores.mainServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>controladores.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dashboardServlet</servlet-name>
        <servlet-class>controladores.dashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminDashboardServlet</servlet-name>
        <servlet-class>controladores.adminDashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajaxAdmin</servlet-name>
        <servlet-class>controladores.ajaxAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>maestroDashboardServlet</servlet-name>
        <servlet-class>controladores.maestroDashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actividadesMaestroServlet</servlet-name>
        <servlet-class>controladores.actividadesMaestroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gruposMaestroServlet</servlet-name>
        <servlet-class>controladores.gruposMaestroServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletAdmin</servlet-name>
        <url-pattern>/servletAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/mainServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboardServlet</servlet-name>
        <url-pattern>/dashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminDashboardServlet</servlet-name>
        <url-pattern>/adminDashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajaxAdmin</servlet-name>
        <url-pattern>/ajaxAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>maestroDashboardServlet</servlet-name>
        <url-pattern>/maestroDashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actividadesMaestroServlet</servlet-name>
        <url-pattern>/actividadesMaestroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gruposMaestroServlet</servlet-name>
        <url-pattern>/gruposMaestroServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
